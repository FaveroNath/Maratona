#include<bits/stdc++.h>
using namespace std;
int main(){
	string num;
	cin >> num;
	int n = 0;
	int tam = num.size();
	if(num[tam-1] == '2' || num[tam-1] == '0' || num[tam-1] == '4' || num[tam-1] == '6' || num[tam-1] == '8') {
	    cout << "S" << endl;
	}
	else{ 
	    cout << "N" << endl;
	}
    for(int i = 0; i < tam; i++){
		n += num[i] - 48;
	}
	if(n%3 == 0){
	cout << "S"<< endl;
	}
	else{
	cout << "N" << endl;
	}
	if(num[tam-1] == '5' || num[tam-1] == '0'){ 
	    cout << "S" << endl;
	}
	else{
	cout << "N";
	}
	return 0;
}
